{
    "name": "waffler/opengen",
    "description": "Waffler client generator using openapi specs",
    "type": "library",
    "require": {
        "waffler/waffler": "^0.5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "mockery/mockery": "^1.4",
        "phpstan/phpstan": "^1.2",
        "jetbrains/phpstorm-attributes": "dev-master",
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Waffler\\Opengen\\": "src/WafflerOpenGen"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waffler\\OpenGen\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "ErickJMenezes",
            "email": "erickmenezes.dev@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
