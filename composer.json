{
    "name": "waffler/opengen",
    "description": "Waffler client generator using openapi specs",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "waffler/waffler": "^1.1.1",
        "cebe/php-openapi": "^1.6.0",
        "symfony/filesystem": "^6.0",
        "nette/php-generator": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "mockery/mockery": "^1.4",
        "phpstan/phpstan": "^1.2",
        "jetbrains/phpstorm-attributes": "dev-master",
        "friendsofphp/php-cs-fixer": "^3.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Waffler\\OpenGen\\": "src/OpenGen/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waffler\\OpenGen\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "./vendor/bin/phpstan",
        "phpunit": "./vendor/bin/phpunit --configuration phpunit.xml",
        "php-cs-fixer": "./vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
    },
    "authors": [
        {
            "name": "ErickJMenezes",
            "email": "erickmenezes.dev@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
